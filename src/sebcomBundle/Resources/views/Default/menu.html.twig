{% extends 'navbar.html.twig' %}
{% block body %}

    <div id="content"></div>

<script type="text/javascript">
    $(document).ready(function(){
        $('a').click(function(e){
            e.preventDefault();
            $("#content").load($(this).attr('href'));
        });
    });
</script>
    {% block javascripts %}{% endblock %}

{% endblock %}

